class Solution {
    public int strStr(String haystack, String needle) {
        
        if(needle.equals("") || needle.equals(haystack)) 
        return 0;
    
    int hLen=haystack.length(), nLen=needle.length();
    
    if(nLen>hLen)
        return -1;
    
    for(int i=0;i<=hLen-nLen;i++){
        if(haystack.substring(i,i+nLen).equals(needle))
            return i;
    }
    
    return -1;
}
}
 
